            [
                "Auto Select",
                "Proxy",
                "direct"
            ],
            "default": "Auto Select"
        },
        {
            "tag": "Apple",
            "type": "selector",
            "outbounds":
            [
                "Auto Select",
                "Proxy",
                "direct"
            ],
            "default": "Auto Select"
